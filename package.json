{
  "name": "resturant-inspections",
  "version": "0.0.1",
  "description": "",
  "engines": {
    "npm": ">=3"
  },
  "scripts": {
    "preinstall": "node tools/nodeVersionCheck.js",
    "setup": "node tools/setup/setupMessage.js && npm install && node tools/setup/setup.js",
    "remove-demo": "babel-node tools/removeDemo.js",
    "start-message": "babel-node tools/startMessage.js",
    "prestart": "npm-run-all --parallel start-message remove-dist",
    "start": "npm-run-all --parallel test:watch open:src lint:watch",
    "open:src": "babel-node tools/srcServer.js",
    "open:dist": "babel-node tools/distServer.js",
    "lint": "esw webpack.config.* src tools --color",
    "lint:watch": "npm run lint -- --watch",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "copy-files": "npm-run-all copy-css copy-imgs copy-js",
    "copy-css": "copyfiles -f src/assets/img/* dist/assets/img/",
    "copy-imgs": "copyfiles -f src/assets/css/* dist/assets/css/",
    "copy-js": "copyfiles -f src/assets/js/* dist/assets/js/",
    "remove-dist": "rimraf ./dist",
    "prebuild": "npm run clean-dist && npm run test  && npm run copy-files",
    "build": "babel-node tools/build.js && npm run open:dist && npm run clean-dist",
    "test": "mocha tools/testSetup.js \"src/**/*.{test, spec}.js\" --spec progress",
    "test:cover": "babel-node node_modules/isparta/bin/isparta cover --root src --report html node_modules/mocha/bin/_mocha -- --require ./tools/testSetup.js \"src/**/*.spec.js\" --reporter progress",
    "test:cover:travis": "babel-node node_modules/isparta/bin/isparta cover --root src --report lcovonly _mocha -- --require ./tools/testSetup.js \"src/**/*.spec.js\" && cat ./coverage/lcov.info | node_modules/coveralls/bin/coveralls.js",
    "test:watch": "npm run test -- --watch",
    "open:cover": "npm run test:cover && open coverage/index.html",
    "deploy": "surge dist kc-restaurant.surge.sh"
  },
  "author": "KCIT",
  "license": "MIT",
  "dependencies": {
    "es6-promise": "4.0.5",
    "google-map-react": "0.19.0",
    "object-assign": "4.1.0",
    "react": "15.3.1",
    "react-dom": "15.3.1",
    "react-gmaps": "1.5.0",
    "react-modal-bootstrap": "1.1.0",
    "react-redux": "4.4.5",
    "react-router": "2.7.0",
    "react-router-redux": "4.0.5",
    "react-scroll": "1.4.4",
    "redux": "3.5.2",
    "redux-thunk": "2.1.0",
    "whatwg-fetch": "2.0.3"
  },
  "devDependencies": {
    "autoprefixer": "6.4.0",
    "babel-cli": "6.14.0",
    "babel-core": "6.14.0",
    "babel-loader": "6.2.5",
    "babel-plugin-react-display-name": "2.0.0",
    "babel-plugin-transform-react-constant-elements": "6.9.1",
    "babel-plugin-transform-react-remove-prop-types": "0.2.9",
    "babel-preset-latest": "6.14.0",
    "babel-preset-react": "6.11.1",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-1": "6.13.0",
    "babel-register": "6.14.0",
    "browser-sync": "2.14.0",
    "chai": "3.5.0",
    "chalk": "1.1.3",
    "connect-history-api-fallback": "1.3.0",
    "copyfiles": "1.0.0",
    "coveralls": "2.11.12",
    "cross-env": "2.0.1",
    "css-loader": "0.24.0",
    "enzyme": "2.4.1",
    "eslint": "3.4.0",
    "eslint-plugin-import": "1.14.0",
    "eslint-plugin-jsx-a11y": "2.2.0",
    "eslint-plugin-react": "6.2.0",
    "eslint-watch": "2.1.14",
    "exports-loader": "0.6.3",
    "extract-text-webpack-plugin": "1.0.1",
    "fetch-mock": "5.10.0",
    "file-loader": "0.9.0",
    "html-webpack-plugin": "2.22.0",
    "imports-loader": "0.6.5",
    "isomorphic-fetch": "2.2.1",
    "isparta": "4.0.0",
    "mocha": "3.0.2",
    "mockdate": "1.0.4",
    "node-sass": "3.8.0",
    "npm-run-all": "3.0.0",
    "open": "0.0.5",
    "postcss-loader": "0.11.0",
    "prompt": "1.0.0",
